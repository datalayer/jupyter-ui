  "scripts": {
    "build": "gulp resources-to-lib && tsc && webpack",
    "build:prod": "gulp resources-to-lib && tsc && webpack --mode=production",
    "clean": "rimraf node_modules lib dist build tsconfig.tsbuildinfo",
    "start": "run-p -c 'start:*'",
    "start:webpack": "webpack serve",
    "start_:jupyter": "cd ./../../dev/sh && ./kill.sh || true && ./start-jupyter-server.sh anonymous",
    "start:tsc": "tsc --watch",
    "build:tsc:watch:tsc": "tsc --watch",
    "build:tsc:watch:res": "gulp resources-to-lib-watch",
    "typedoc": "typedoc ./src",
    "watch": "tsc --watch",
    "start_:jupyterpool": "cd dist && jupyterpool --port 3266 --ServerApp.jpserver_extensions=\"{'jupyterlab': True, 'jupyterpool': True,  'nbclassic': True }\"",
    "kill": "./../dev/sh/kill.sh || true"
  },
