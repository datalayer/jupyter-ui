    "@mui/styles": "5.10.7",
    "@mui/system": "5.10.7",
    "@types/codemirror": "5.60.4",
    "@types/jest": "29.4.0",
    "@types/marked": "4.0.1",
    "@types/node": "18.15.3",
    "@types/plotly.js": "2.12.25",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/styled-components": "5.1.26",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "babel-loader": "9.1.2",
    "bundle-loader": "0.5.6",
    "css-loader": "5.2.4",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "file-loader": "~5.0.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "gulp-filter": "6.0.0",
    "gulp-watch": "5.0.1",
    "html-webpack-plugin": "5.3.1",
    "html-webpack-tags-plugin": "2.0.17",
    "jest": "29.4.3",
    "mkdirp": "1.0.3",
    "npm-run-all": "4.1.5",
    "plotly.js": "2.25.0",
    "prettier": "2.1.1",
    "process": "0.11.10",
    "raw-loader": "~4.0.0",
    "rimraf": "3.0.2",
    "stream-browserify": "2.0.2",
    "style-loader": "2.0.0",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-prettier": "2.0.0",
    "svg-url-loader": "~3.0.3",
    "ts-jest": "29.0.5",
    "typedoc": "0.24.4",
    "typescript": "5.0.3",
    "url-loader": "~3.0.0",
    "watch": "~1.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "sideEffects": [
    "style/*.css",
    "src/**/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_react"
        }
      }
    },
    "extension": "./lib/jupyter/lab/index.js",
    "outputDir": "jupyter_react/labextension",
    "schemaDir": "schema"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab==4.0.3",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
