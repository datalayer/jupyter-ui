    "@typescript-eslint/parser": "4.8.1",
    "babel-loader": "8.3.0",
    "bundle-loader": "0.5.6",
    "css-loader": "5.2.4",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "file-loader": "~5.0.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "gulp-filter": "6.0.0",
    "gulp-watch": "5.0.1",
    "html-webpack-plugin": "5.3.1",
    "html-webpack-tags-plugin": "2.0.17",
    "jest": "26.0.0",
    "mkdirp": "1.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "process": "^0.11.10",
    "raw-loader": "~4.0.0",
    "rimraf": "~3.0.0",
    "stream-browserify": "2.0.2",
