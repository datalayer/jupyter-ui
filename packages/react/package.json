    "@jupyterlab/terminal": "4.0.0-alpha.18",
    "@jupyterlab/theme-dark-extension": "4.0.0-alpha.18",
    "@jupyterlab/theme-light-extension": "4.0.0-alpha.18",
    "@lumino/default-theme": "1.0.0-alpha.6",
    "@primer/octicons-react": "17.10.0",
    "@primer/react": "35.18.0",
    "assert": "2.0.0",
    "codemirror": "6.0.1",
    "lodash": "^4.17.4",
    "marked": "4.0.10",
    "plotly.js": "2.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.3",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-observable": "1.2.0",
    "rxjs": "6.6.0",
    "stream": "0.0.2",
    "styled-components": "4.4.1",
    "typescript-fsa": "3.0.0",
    "typescript-fsa-reducers": "1.2.1",
    "uuid": "8.3.2"
  },
