    "@jupyterlab/filebrowser": "4.0.0-alpha.18",
    "@jupyterlab/fileeditor": "4.0.0-alpha.18",
    "@jupyterlab/javascript-extension": "4.0.0-alpha.18",
    "@jupyterlab/json-extension": "4.0.0-alpha.18",
