    "watch": "~1.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
        "server": {
          "managers": [
            "pip"
          ],
          "base": {
            "name": "jupyter_react"
          }
        }
    },
    "extension": "./lib/jupyter/lab/index.js",
    "outputDir": "jupyter_react/labextension",
    "schemaDir": "schema"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab==4.0.0a33",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
