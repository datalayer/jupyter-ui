{
  "name": "@datalayer/jupyter-embed",
  "version": "0.1.0",
  "description": "Easily embed Jupyter components (cells, notebooks, terminals, consoles) into any web page",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "dist/**/*",
    "style/**/*.{css,js}"
  ],
  "keywords": [
    "jupyter",
    "jupyterlab",
    "embed",
    "react",
    "widget",
    "notebook",
    "cell",
    "terminal",
    "console",
    "datalayer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer/jupyter-ui.git",
    "directory": "packages/embed"
  },
  "homepage": "https://github.com/datalayer/jupyter-ui/tree/main/packages/embed",
  "bugs": {
    "url": "https://github.com/datalayer/jupyter-ui/issues"
  },
  "sideEffects": [
    "style/*.css",
    "src/**/*.css",
    "style/index.js",
    "dist/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "tsc",
    "build:dist": "vite build",
    "build:prod": "npm run clean && npm run build",
    "clean": "rimraf lib dist tsconfig.tsbuildinfo",
    "dev": "vite",
    "lint": "eslint . --cache --ext .ts,.tsx --fix",
    "lint:check": "eslint . --cache --ext .ts,.tsx",
    "preview": "vite preview",
    "test": "jest --coverage",
    "typedoc": "typedoc ./src",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@datalayer/jupyter-react": "^2.0.0",
    "@datalayer/primer-addons": "^1.0.4",
    "@primer/react": "^36.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^9.0.0",
    "rimraf": "^6.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-css-injected-by-js": "^3.0.0"
  }
}
