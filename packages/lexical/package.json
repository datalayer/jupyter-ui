{
  "name": "@datalayer/jupyter-lexical",
  "version": "1.0.6",
  "description": "Jupyter UI for Lexical",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "keywords": [
    "jupyter",
    "react",
    "lexical"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer/jupyter-ui.git",
    "directory": "packages/lexical"
  },
  "sideEffects": [
    "style/*.css",
    "src/**/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run validate:tools && gulp resources-to-lib && cross-env NODE_OPTIONS=--max-old-space-size=16384 tsc && webpack && npm run build:lib",
    "build:lib": "npm run validate:tools && cross-env NODE_OPTIONS=--max-old-space-size=16384 tsc",
    "build:prod": "npm run validate:tools && gulp resources-to-lib && tsc && jlpm clean && jlpm build:lib",
    "build:tsc:watch": "run-p 'build:tsc:watch:*'",
    "build:tsc:watch:res": "gulp resources-to-lib-watch",
    "build:tsc:watch:tsc": "tsc --watch",
    "clean": "rimraf node_modules lib dist build tsconfig.tsbuildinfo",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "clean:labextension": "rimraf datalayer/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "dev": "run-p -c 'start:*'",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "kill": "./../../dev/sh/kill.sh || true",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "start": "run-p -c 'start:*'",
    "start_:jupyter": "cd ./../.. && make start-jupyter-server",
    "start:webpack": "webpack serve",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "typedoc": "typedoc ./src",
    "validate:tools": "node scripts/validate-tools-sync.js",
    "watch": "run-p watch:src",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@datalayer/icons-react": "^1.0.0",
    "@datalayer/jupyter-react": "^1.1.8",
    "@datalayer/primer-addons": "^1.0.4",
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/coreutils": "^6.0.0",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/settingregistry": "^4.0.0",
    "@lexical/code": "^0.35.0",
    "@lexical/link": "^0.35.0",
    "@lexical/list": "^0.35.0",
    "@lexical/mark": "^0.35.0",
    "@lexical/react": "^0.35.0",
    "@lexical/rich-text": "^0.35.0",
    "@lexical/selection": "^0.35.0",
    "@lexical/table": "^0.35.0",
    "@lexical/utils": "^0.35.0",
    "@lexical/yjs": "^0.35.0",
    "autoprefixer": "^10.4.21",
    "katex": "^0.16.21",
    "lexical": "^0.35.0",
    "lodash-es": "^4.17.21",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-json-tree": "^0.19.0",
    "styled-components": "^5.3.10",
    "tailwindcss": "^4.1.11",
    "y-websocket": "3.0.0",
    "yjs": "^13.6.24"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/codemirror": "^5.60.4",
    "@types/katex": "^0.14.0",
    "@types/lodash-es": "^4.17.6",
    "@types/marked": "^4.0.1",
    "@types/node": "^22.14.1",
    "@types/plotly.js": "^1.54.11",
    "@types/prettier": "^3.0.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-loader": "^9.1.2",
    "bundle-loader": "^0.5.6",
    "css-loader": "^7.1.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "^1.0.8",
    "gulp-filter": "^6.0.0",
    "gulp-watch": "^5.0.1",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-tags-plugin": "^3.0.2",
    "jest": "^29.4.3",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "raw-loader": "^4.0.2",
    "react-is": "^19.2.0",
    "rimraf": "^6.0.1",
    "stream": "^0.0.2",
    "stream-browserify": "^2.0.2",
    "style-loader": "^4.0.0",
    "stylelint": "^16.26.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "typedoc": "^0.28.2",
    "typescript": "^5.8.3",
    "url-loader": "^3.0.0",
    "watch": "^1.0.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
