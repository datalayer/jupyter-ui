    "@jupyterlab/coreutils": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.19.4",
    "@types/jest": "29.4.0",
    "jest": "29.4.3",
    "rimraf": "~3.0.0",
    "ts-jest": "29.0.5",
    "typescript": "5.0.3"
  },
