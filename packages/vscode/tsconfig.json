{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "vscpp",
    "jsxFragmentFactory": "vscppf",
    "module": "ESNext",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "types": ["node", "mocha"],
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "webview"]
}
