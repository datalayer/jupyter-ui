{
  "name": "@datalayer/jupyterlite-ipykernel",
  "version": "0.1.0-beta.13",
  "description": "JupyterLite - Pyolite Kernel",
  "homepage": "https://github.com/jupyterlite/jupyterlite",
  "bugs": {
    "url": "https://github.com/jupyterlite/jupyterlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlite/jupyterlite.git"
  },
  "license": "BSD-3-Clause",
  "author": "JupyterLite Contributors",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/",
    "py": "py/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js",
    "pypi/*.{json,whl}"
  ],
  "scripts": {
    "build": "yarn run build:lib",
    "build:lib": "tsc -b",
    "build:python": "python -m pip install build && cd py && python -m build -w",
    "build:test": "tsc --build tsconfig.test.json",
    "clean": "yarn run clean:lib && yarn run clean:python",
    "clean:lib": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "clean:python": "rimraf py/**/dist",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@datalayer/jupyterlite-kernel": "0.1.0-beta.13",
    "comlink": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.19.4",
    "@jupyterlab/testutils": "4.0.0-beta.1",
    "@types/jest": "29.4.0",
    "jest": "29.4.3",
    "rimraf": "~3.0.0",
    "ts-jest": "29.0.5",
    "typescript": "5.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "pyolite": {
    "packages": {
      "py/pyolite": "0.1.0b12",
      "py/piplite": "0.1.0b12",
      "py/ipykernel": "6.9.2",
      "py/widgetsnbextension": "3.6.0"
    }
  },
  "styleModule": "style/index.js"
}
