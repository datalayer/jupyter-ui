{
  "name": "datalayer-jupyter-vscode",
  "displayName": "Datalayer Notebook",
  "description": "A Jupyter Notebook Editor for VS Code",
  "publisher": "datalayer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Data Science"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "datalayer.notebook.new",
        "title": "Create new Datalayer Notebook",
        "category": "Notebook"
      }
    ],
    "customEditors": [
      {
        "viewType": "datalayer.notebook",
        "displayName": "Datalayer Notebook",
        "selector": [
          {
            "filenamePattern": "*.ipynb"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
